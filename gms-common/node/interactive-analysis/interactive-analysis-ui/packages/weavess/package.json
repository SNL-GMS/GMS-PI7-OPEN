{
  "name": "@gms/weavess",
  "author": "GMS Development Team",
  "version": "1.3.0",
  "description": "WEAVESS: Web Enhanced Application for the Viewing & Exploitation of Sensor Samples, a library for high-performance waveform display",
  "license": "UNLICENSED",
  "main": "./dist/lib/weavess.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json 'src/**/*.{ts,tsx}' --fix",
    "precommit": "lint-staged",
    "clean": "rm -rf ./build/* && rm -rf ./build/*",
    "copy-scss": "cd ./src/ts/ && copyfiles \"./**/*.scss\" \"../../dist/lib/\"",
    "build-lib": "tsc --project ./tsconfig.json && npm run copy-scss",
    "build-dts": "dts-bundle --name weavess --main ./dist/types/weavess.d.ts --out ./weavessmain.d.ts",
    "build": "npm run clean && npm run lint && npm run build-lib && npm run build-dts",
    "build-bundle": "npm run clean && webpack --colors --progress --env production --config webpack.config.js",
    "build-bundle-min": "npm run clean && webpack --output-filename ./dist/weavess.min.js --optimize-minimize --colors --progress --env production --config webpack.config.js",
    "start": "npm run build && tsc --watch --pretty --project ./tsconfig.json",
    "dev": "webpack-dev-server --colors --progress --env development",
    "prod": "webpack-dev-server --colors --progress --env production",
    "test": "jest --config jest.config.js",
    "sonar": "echo \"run 'npm run sonar' from the top-level directory\"",
    "version": "echo $npm_package_version",
    "docs": "typedoc --out docs ./src/ --name weavess --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "2.3.1",
    "@blueprintjs/datetime": "2.0.3",
    "@blueprintjs/icons": "2.2.1",
    "d3": "5.5.0",
    "element-resize-event": "2.0.9",
    "lodash": "4.17.10",
    "moment": "2.22.1",
    "moment-precise-range-plugin": "1.3.0",
    "memoize-one": "4.0.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.5.1",
    "three": "0.98.0",
    "worker-rpc": "0.1.0"
  },
  "devDependencies": {
    "@blueprintjs/tslint-config": "1.3.1",
    "@types/d3": "4.12.0",
    "@types/enzyme": "3.1.14",
    "@types/faker": "4.1.4",
    "@types/jest": "23.3.3",
    "@types/lodash": "4.14.107",
    "@types/react": "16.7.17",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "4.3.1",
    "@types/three": "0.93.10",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "copyfiles": "2.1.0",
    "css-loader": "0.28.11",
    "dts-bundle": "0.7.3",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.4",
    "faker": "4.1.0",
    "fork-ts-checker-webpack-plugin": "0.4.1",
    "highlight.js": "9.13.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-styled-components": "6.2.1",
    "jsdom": "11.8.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "7.0.5",
    "math-random": "1.0.4",
    "node-sass": "4.9.0",
    "prettier": "1.12.1",
    "react-test-renderer": "16.3.2",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "transform-loader": "0.2.4",
    "ts-jest": "23.10.3",
    "ts-loader": "4.2.0",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.12.0",
    "tslint-eslint-rules": "5.1.0",
    "tslint-language-service": "0.9.9",
    "tslint-react": "3.6.0",
    "typedoc": "0.14.2",
    "typescript": "3.1.1",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2",
    "worker-loader": "1.1.1"
  },
  "peerDependencies": {
    "worker-loader": "1.1.1"
  },
  "typings": "./dist/types/weavess.d.ts"
}
