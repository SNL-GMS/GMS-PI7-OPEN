  "main": "build/js/src/ts/main.js",
  "scripts": {
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json 'src/**/*.{ts,tsx}' --fix",
    "precommit": "lint-staged",
    "clean": "rm -rf ./build/* && rm -rf ./build/*",
    "build": "npm run clean && npm run lint && webpack --colors --progress --env production --config webpack.config.js && tsc",
    "dev": "npm run lint && webpack-dev-server --colors --progress --env development",
    "prod": "npm run lint && webpack-dev-server --colors --progress --env production",
    "start": "npm run prod",
    "test": "jest --config jest.config.js --maxWorkers 2",
    "sonar": "echo \"run 'npm run sonar' from the top-level directory\"",
    "docs": "typedoc --out docs ./src/ --name analyst-ui-core --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "__tests__/**/*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "__mocks__/**/*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ]
  },
