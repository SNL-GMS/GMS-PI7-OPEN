{
	"name": "interactive-analysis-ui",
	"version": "7.5.1",
	"description": "GMS Interactive Analysis",
	"license": "UNLICENSED",
	"scripts": {
		"version": "echo $npm_package_version",
		"bootstrap": "lerna bootstrap",
		"build": "lerna run build",
		"ci-install-build": "npm install && npm run bootstrap && npm run build",
		"test": "lerna run test",
		"test-jest": "jest --config jest.config.js",
		"sonar": "sonar-scanner -Dsonar.sources=packages/analyst-ui-core/src/ts,packages/analyst-ui-electron/src/ts,packages/ui-core-components/src/ts,packages/weavess/src/ts -Dsonar.projectKey=interactive-analysis-ui -Dsonar.host.url=https://example.com/sonar -Dsonar.typescript.lcov.reportPaths=packages/analyst-ui-core/coverage/lcov.info,packages/analyst-ui-electron/coverage/lcov.info,packages/ui-core-components/coverage/lcov.info,packages/weavess/coverage/lcov.info",
		"dev": "cd packages/analyst-ui-core && npm run dev",
		"prod": "cd packages/analyst-ui-core && npm run prod",
		"start": "npm run prod",
		"clean": "lerna clean && rm -rf ./node_modules",
		"docs": "rm -rf ./docs && mkdir ./docs && npm run docs-interactive-analysis && mkdir ./docs/packages && npm run docs-analyst-ui-core && cp -r ./packages/analyst-ui-core/docs ./docs/packages/analyst-ui-core && npm run docs-analyst-ui-electron && cp -r ./packages/analyst-ui-electron/docs ./docs/packages/analyst-ui-electron  && npm run docs-ui-core-components && cp -r ./packages/ui-core-components/docs ./docs/packages/ui-core-components && npm run docs-weavess && cp -r ./packages/weavess/docs ./docs/packages/weavess",
		"docs-interactive-analysis": "typedoc --out docs ./.gitlab --name interactive-analysis --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator",
		"docs-analyst-ui-core": "cd ./packages/analyst-ui-core && npm run docs",
		"docs-analyst-ui-electron": "cd ./packages/analyst-ui-electron && npm run docs",
		"docs-ui-core-components": "cd ./packages/ui-core-components && npm run docs",
		"docs-weavess": "cd ./packages/weavess && npm run docs"
	},
	"devDependencies": {
		"@types/enzyme": "3.1.14",
		"@types/faker": "4.1.4",
		"@types/jest": "23.3.3",
		"@types/react": "16.4.18",
		"@types/react-dom": "16.0.9",
		"apollo-link-schema": "1.1.0",
		"enzyme": "3.6.0",
		"enzyme-adapter-react-16": "1.6.0",
		"enzyme-to-json": "3.3.4",
		"faker": "4.1.0",
		"jest": "23.6.0",
		"jest-canvas-mock": "1.1.0",
		"jest-cli": "23.6.0",
		"jsdom": "11.8.0",
		"jsdom-global": "3.0.2",
		"lerna": "3.10.8",
		"node-fetch": "2.2.0",
		"react": "16.6.3",
		"react-apollo": "2.2.4",
		"react-dom": "16.6.3",
		"sonar-scanner": "3.1.0",
		"ts-jest": "23.10.3",
		"typescript": "3.1.1"
	},
	"dependencies": {
		"npm-snapshot": "1.0.3",
		"highlight.js": "9.13.1",
		"typedoc": "0.14.2"
	}
}
